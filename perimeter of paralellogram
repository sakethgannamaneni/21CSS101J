#include <stdio.h>
int main(){
    float num1, num2, perimeter;

    printf("Enter the length of adjacent sides of the parallelogram: ");
    scanf("%f %f", &num1, &num2);

    perimeter = 4 * (num1 + num2);

    printf("Perimeter of the parallelogram is: %.2f", perimeter);

    return 0;
}
