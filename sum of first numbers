#include<stdio.h>
void main(){
  
  printf("printing sum of first n natural numbers.\n");
  int num to add;
printf("number of numbers to be added : ");
  scanf("%d",&num to add);
  int sum = 0;
  for(int number = 1; number <= num to add; number ++) { sum = sum + number;}
  printf("sum of first %d numbers is %d", num to add,sum);
}
